{
  "name": "webpack-sample",
  "version": "0.0.1",
  "scripts": {
    "build:dev": "npm run clean:dist && npm run webpack -- --config webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && npm run webpack -- --config webpack.prod.js --bail",
    "ci": "npm run lint && npm run test && npm run build:prod",
    "clean:dist": "npx rimraf -- dist",
    "lint": "npx eslint ./src",
    "server:dev": "npm run webpack serve -- --config webpack.dev.js --progress",
    "start": "npm run server:dev",
    "test": "npx karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "webpack": "node --max_old_space_size=8192 node_modules/webpack/bin/webpack.js"
  },
  "license": "MIT",
  "dependencies": {
    "normalize.css": "8.0.1",
    "socket.io-client": "4.7.5"
  },
  "devDependencies": {
    "@types/jasmine": "5.1.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "10.4.19",
    "circular-dependency-plugin": "5.2.2",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "6.10.0",
    "eslint": "8.57.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "import-glob-loader": "1.1.0",
    "jasmine-core": "5.1.2",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-jasmine": "5.1.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-webpack": "5.0.1",
    "mini-css-extract-plugin": "2.8.1",
    "postcss": "8.4.38",
    "postcss-loader": "8.1.1",
    "rimraf": "5.0.5",
    "sass": "1.72.0",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "ts-loader": "9.5.1",
    "typescript": "5.4.3",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-merge": "5.10.0"
  }
}
